    <% layout('../layout') -%>
    <% script('https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.4.0/Chart.js') -%>
    <% script('/js/results.front.js') -%>

<h1>Resultats</h1>
<div class="containter">
    <% questions.forEach(function(question){%>
        <h2><%= question.intitule %></h2>
        <div>
            <canvas class="charts" data-question-id="<%= question.id %>" data-reponse-nb="<%= question.reponses.length %>" id="chart-<%= question.id %>" width="400" height="400"></canvas>
        </div>
    <% }); %>
</div>
